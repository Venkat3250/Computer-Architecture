1. #include<stdio.h> 
2. #include<stdlib.h> 
3. int main(){ 
4. int a[10],n,i; 
5. system ("cls"); 
6. printf("Enter the number to convert: "); 
7. scanf("%d",&n); 
8. for(i=0;n>0;i++) 
9. { 
10. a[i]=n%2; 
11. n=n/2; 
12. } 
13. printf("\nBinary of Given Number is="); 
14. for(i=i-1;i>=0;i--) 
15. { 
16. printf("%d",a[i]); 
17. } 
18. return 0; 
19. } 
